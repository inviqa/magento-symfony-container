{
    "name": "inviqa/magento-symfony-container",
    "type": "magento-module",
    "license": "proprietary",
    "description": "Provides Magento with an instance of a Symfony DI Container",
    "require": {
        "inviqa/symfony-container-generator": "^1.0",
        "magento-hackathon/magento-composer-installer": "^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "app/code/community"
        }
    },
    "extra": {
        "magento-version-ee": "^1.12.0",
        "magento-root-dir": "public"
    }
}
